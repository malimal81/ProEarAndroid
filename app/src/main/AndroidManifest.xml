<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jamaljones.proear">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:largeHeap="true">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Interval_Recognition"
            android:label="@string/title_activity_interval__recognition"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Interval_Playback"
            android:label="@string/title_activity_interval__playback"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Pitch_Recognition"
            android:label="@string/title_activity_pitch__recognition"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Melody_Playback"
            android:label="@string/title_activity_melody__playback"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Chord_Recognition"
            android:label="@string/title_activity_chord__recognition"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Chord_Inversions"
            android:label="@string/title_activity_chord__inversions"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>
